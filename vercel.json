{
  "version": 2,
  "builds": [
    {
      "src": "./server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "./client/dist",
      "use": "@vercel/static-site"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}
